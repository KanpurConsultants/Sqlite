        Dim RepName As String = "", RepTitle As String = ""
        Dim bCondstr As String = ""
        Dim mJobOn$ = ""

        Try
            Me.Cursor = Cursors.WaitCursor

            mQry = " Select JobOn From Process Where NCat = '" & TxtProcess.Tag & "'"
